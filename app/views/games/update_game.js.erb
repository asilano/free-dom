<% if (!@just_checking || !@player || !@player.lock) %>
  $("#flash").html('<%= escape_javascript(render('flash_area')) -%>');
  $("#game").html('<%= escape_javascript(render(@game, :player => @player, :controls => @controls)) -%>');
  <%= render :partial => 'update_chat' %>
  onLoadFunc();

  <% if @full_title -%>
    document.title = "<%= escape_javascript(@full_title) -%>";
  <% end %>
<% end %>